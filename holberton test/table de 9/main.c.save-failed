#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int j;

    for (i = 0; i < 10 ; i++)
    {
        for (j = 0; j < 10 ; j ++)
        {
            if ((i * j) < 10)
            {
                printf("%2d, ", i * j);
            }else if ((i * j) >= 10)
            {
                 if (!((i * j) == 82))
                 printf("%d, ", i * j);
            }
        }
        printf("\n");
    }
}
